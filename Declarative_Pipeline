pipeline{
    agent any
    
    environment{
        PATH = "/bin:$PATH"
    }
    stages{
        stage("GitCheckOut"){
            steps{
                
                git branch: 'main', credentialsId: 'github', url: 'https://github.com/ashisharyaa/DevOpsProject.git'
                
            }
        }
        
        stage("Maven Build"){
            steps{
                
                sh "mvn clean package"
                
            }
        }
        
    }
    
    
}
