pipeline{
    agent any
    
    
    stages{
        stage("GitCheckOut"){
            steps{
                
                git branch: 'main', credentialsId: 'git', url: 'https://github.com/ashisharyaa/today.git'
                
            }
        }
        
        stage("Maven Build"){
            steps{
                
                sh "mvn clean package"
                
            }
        }
        
    }
    
    
}
